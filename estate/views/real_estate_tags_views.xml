<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.actions.act_window" id="estate_tags_action">
        <field name="name">Tags action</field>
        <field name="res_model">real.estate.tags</field>
        <field name="view_mode">list</field>
    </record>

    <record id="real_estate_tags_view_list" model="ir.ui.view" >
        <field name="name">real.estate.tags.view.list</field>
        <field name="model">real.estate.tags</field>
        <field name="arch" type="xml">
            <list>
                <field name="name" />
                <field name="color" />
            </list>
        </field>
    </record>

</odoo>